+void
+MacLow::SetChannelWidthForRu(WifiTxVector &txVector)
+{
+  HEBitMap bitmap;
+  RUInfo ruI = bitmap.GetRUInfoFromTriggerBitMap(txVector.GetRu());
+  uint32_t chanW = 20;
+  if (ruI.type == 1)
+    chanW = 2;
+  else if (ruI.type == 2)
+    chanW = 4;
+  else if (ruI.type == 3)
+    chanW = 8;
+  else if (ruI.type == 4)
+    chanW = 20;
+  else if (ruI.type == 5)
+    chanW = 40;
+  else if (ruI.type == 6)
+    chanW = 80;
+  else if (ruI.type == 7)
+    chanW = 160;
+  txVector.SetChannelWidth(chanW);
+}
